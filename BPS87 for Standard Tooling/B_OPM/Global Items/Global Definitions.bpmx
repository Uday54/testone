          itemSubjectRef="_GWk2MMT2EeGIlOhhn7SrtQ:_GZHM1MT2EeGIlOhhn7SrtQ"/>
      <bpmn2:inputSet id="_HSoO_sT2EeGIlOhhn7SrtQ">
        <bpmn2:dataInputRefs>_PSfBaMT2EeGIlOhhn7SrtQ</bpmn2:dataInputRefs>
      </bpmn2:inputSet>
      <bpmn2:outputSet id="_HSoO_8T2EeGIlOhhn7SrtQ">
        <bpmn2:dataOutputRefs>_PSfBacT2EeGIlOhhn7SrtQ</bpmn2:dataOutputRefs>
      </bpmn2:outputSet>
    </bpmn2:ioSpecification>
  </bpmn2:globalTask>
  <bpmn2:globalTask id="_HSoPAMT2EeGIlOhhn7SrtQ" name="Enact Asset Transfer">
    <bpmn2:documentation id="_HSoPAcT2EeGIlOhhn7SrtQ">SHORT NAME: Enact Asst Trnsfr&#xD;
&#xD;
B LEVEL ROOT: Enact Sale&#xD;
&#xD;
DEFINITION: To perform or carry out an ownership transfer of existing assets from one balance sheet to another.&#xD;
 &#xD;
DESCRIPTION: Enact the actual transfer of existing assets from one Institution to another; this can be done by means of a &quot;True Sale&quot;.</bpmn2:documentation>
