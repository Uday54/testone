      </bpmn2:inputSet>
      <bpmn2:outputSet id="_HSoPBMT2EeGIlOhhn7SrtQ">
        <bpmn2:dataOutputRefs>_PSfBa8T2EeGIlOhhn7SrtQ</bpmn2:dataOutputRefs>
      </bpmn2:outputSet>
    </bpmn2:ioSpecification>
  </bpmn2:globalTask>
  <bpmn2:globalTask id="_HSoPBcT2EeGIlOhhn7SrtQ" name="Modify Securitization Plan Details">
