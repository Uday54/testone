          itemSubjectRef="_GWk2MMT2EeGIlOhhn7SrtQ:_GYXl8MT2EeGIlOhhn7SrtQ"/>
      <bpmn2:inputSet id="_GawLqMT2EeGIlOhhn7SrtQ">
        <bpmn2:dataInputRefs>_PNt2g8T2EeGIlOhhn7SrtQ</bpmn2:dataInputRefs>
      </bpmn2:inputSet>
      <bpmn2:outputSet id="_GawLqcT2EeGIlOhhn7SrtQ">
        <bpmn2:dataOutputRefs>_PNt2hcT2EeGIlOhhn7SrtQ</bpmn2:dataOutputRefs>
