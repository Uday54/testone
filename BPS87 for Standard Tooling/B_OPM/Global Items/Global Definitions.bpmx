      <bpmn2:dataOutput id="_PU3m68T2EeGIlOhhn7SrtQ" name="Accounting Unit" isCollection="false"
          itemSubjectRef="_GWk2MMT2EeGIlOhhn7SrtQ:_GYXl8MT2EeGIlOhhn7SrtQ"/>
      <bpmn2:inputSet id="_H3bk98T2EeGIlOhhn7SrtQ">
        <bpmn2:dataInputRefs>_PU3m6cT2EeGIlOhhn7SrtQ</bpmn2:dataInputRefs>
        <bpmn2:dataInputRefs>_PU3m6sT2EeGIlOhhn7SrtQ</bpmn2:dataInputRefs>
      </bpmn2:inputSet>
      <bpmn2:outputSet id="_H3bk-MT2EeGIlOhhn7SrtQ">
