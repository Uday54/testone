      <bpmn2:inputSet id="_G8IaYsT2EeGIlOhhn7SrtQ">
        <bpmn2:dataInputRefs>_PQ2CqMT2EeGIlOhhn7SrtQ</bpmn2:dataInputRefs>
        <bpmn2:dataInputRefs>_PQ2CqcT2EeGIlOhhn7SrtQ</bpmn2:dataInputRefs>
      </bpmn2:inputSet>
      <bpmn2:outputSet id="_G8IaY8T2EeGIlOhhn7SrtQ">
