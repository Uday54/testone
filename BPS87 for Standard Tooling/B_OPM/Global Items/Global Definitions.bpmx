      <bpmn2:inputSet id="_H3SbB8T2EeGIlOhhn7SrtQ">
        <bpmn2:dataInputRefs>_PUt2G8T2EeGIlOhhn7SrtQ</bpmn2:dataInputRefs>
      </bpmn2:inputSet>
      <bpmn2:outputSet id="_H3SbCMT2EeGIlOhhn7SrtQ">
